<widget-definition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
						 xsi:noNamespaceSchemaLocation="http://www.hybris.com/schema/cockpitng/widget-definition.xsd"
						 id="de.hybris.platform.datahubbackoffice.quickUpload">

	<name>QuickUpload</name>
	<description>Import, Compose and Publish from a single widget</description>
	<defaultTitle>Quick Upload</defaultTitle>
	<author>hybris</author>
	<version>1.0</version>

	<controller class="de.hybris.platform.datahubbackoffice.presentation.widgets.quickupload.QuickUploadController"/>

	<sockets>
		<input id="datahubInfo" type="de.hybris.platform.datahubbackoffice.service.datahub.DataHubServerInfo"/>
	</sockets>

	<settings>
		<setting key="quickUploadTitle" default-value="" type="String"/>
		<setting key="editorPageSize" default-value="10" type="Integer"/>
	</settings>

	<keywords>
		<keyword>datahub</keyword>
	</keywords>

</widget-definition>