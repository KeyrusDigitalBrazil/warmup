#
# Import the English CMS content for the Apparel Site UK
#

$contentCatalog=apparel-ukContentCatalog
$contentCV=catalogVersion(CatalogVersion.catalog(Catalog.id[default=$contentCatalog]),CatalogVersion.version[default=Staged])[default=$contentCatalog:Staged]

# Language
$lang=en


    
# Content Pages
UPDATE ContentPage;$contentCV[unique=true];uid[unique=true];title[lang=$lang]
 ;;add-edit-address;"Add/Edit Address"
 ;;address-book;"Address Book"
 ;;cartPage;"Your Shopping Bag"
 ;;checkout-login;"Proceed to Checkout"
 ;;close-account;"Close Account Page"
 ;;consents;"Consent Management Page"
 ;;faq;"Frequently Asked Questions"
 ;;homepage;"Homepage"
 ;;importCSVSavedCartPage;"Import Saved Cart"
 ;;login;"Login"
 ;;multiStepCheckoutSummaryPage;"Checkout"
 ;;notFound;"Not Found"
 ;;order;"Order Details"
 ;;orderConfirmationPage;"Order Confirmation"
 ;;orderExpired;"Order Expired Page"
 ;;orders;"Order History"
 ;;payment-details;"Payment Details"
 ;;quickOrderPage;"Quick Order Page"
 ;;saved-carts;"Saved Carts"
 ;;savedCartDetailsPage;"Saved Cart Details"
 ;;search;"Search Results Page"
 ;;searchEmpty;"No Results"
 ;;storefinderPage;"StoreFinder"
 ;;termsAndConditions;"Terms and Conditions"
 ;;update-email;"Update Email Page"
 ;;update-profile;"Update Profile Page"
 ;;updatePassword;"Update Forgotten Password"

        
# Category Pages
UPDATE CategoryPage;$contentCV[unique=true];uid[unique=true];title[lang=$lang]
 ;;BrandsCategoryPage;"Brands Category Page"
 ;;ShirtsCategoryPage;"Shirts Category Page"
 ;;SnowCategoryPage;"Snow Category Page"
 ;;StreetCategoryPage;"Street Category Page"
 ;;SunglassesCategoryPage;"Sunglasses Category Page"
 ;;SurfCategoryPage;"Surf Category Page"
 ;;category;"Default Category Page"
 ;;productGrid;"Product Grid"
 ;;productList;"Product List"

        
# Product Pages
UPDATE ProductPage;$contentCV[unique=true];uid[unique=true];title[lang=$lang]
 ;;productDetails;"Product Details"

    