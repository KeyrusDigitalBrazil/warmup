		parent="abstractItemDao" />

	<alias alias="cmsDataFactory" name="defaultCMSDataFactory" />
	<bean id="defaultCMSDataFactory" class="de.hybris.platform.cms2.servicelayer.data.impl.DefaultCMSDataFactory">
		<property name="cmsSiteService" ref="cmsSiteService" />
		<property name="productService" ref="productService" />
		<property name="categoryService" ref="categoryService" />
		<property name="catalogService" ref="catalogService" />
	</bean>

	<bean id="cockpitRestrictedSystemProperties" class="org.springframework.beans.factory.config.ListFactoryBean">
		<property name="sourceList">
			<list>
				<value>allDocuments</value>
				<value>assignedCockpitItemTemplates</value>
				<value>catalogVersion</value>
				<value>container</value>
				<value>containers</value>
				<value>creationtime</value>
				<value>itemtype</value>
				<value>modifiedtime</value>
				<value>name</value>
				<value>owner</value>
				<value>pk</value>
				<value>savedValues</value>
				<value>slots</value>
				<value>synchronizationSources</value>
				<value>synchronizedCopies</value>
				<value>type</value>
				<value>typeCode</value>
				<value>uid</value>
				<value>visible</value>
				<value>dimVals</value>
				<value>onlyOneRestrictionMustApply</value>
				<value>comments</value>
			</list>
		</property>
	</bean>

