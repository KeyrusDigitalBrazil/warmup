<project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://maven.apache.org/POM/4.0.0"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>de.hybris.platform</groupId>
	<artifactId>integrationservices</artifactId>
	<version>18.11.0-RC11-SNAPSHOT</version>
	<properties>
		<sap.passport.version>1.2.0</sap.passport.version>
	</properties>

	<packaging>jar</packaging>

	<dependencies>
		<dependency>
			<groupId>com.sap.core.jdsr</groupId>
			<artifactId>com.sap.js.passport.api</artifactId>
			<version>${sap.passport.version}</version>
		</dependency>
	</dependencies>

	<repositories>
		<repository>
			<id>sap-release</id>
			<url>http://nexusrel.wdf.sap.corp:8081/nexus/content/groups/releases.all/</url>
			<releases>
				<enabled>true</enabled>
				<updatePolicy>daily</updatePolicy>
				<checksumPolicy>fail</checksumPolicy>
			</releases>
			<snapshots>
				<enabled>false</enabled>
			</snapshots>
		</repository>
	</repositories>
</project>
