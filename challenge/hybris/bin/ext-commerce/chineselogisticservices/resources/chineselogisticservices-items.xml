                </attribute>
            </attributes>
		</itemtype>
		<itemtype code="AbstractOrder" autocreate="false" generate="false">
			<description>additional attributes.</description>
			<attributes>
				<attribute qualifier="deliveryTimeSlot" type="DeliveryTimeSlot">
					<persistence type="property" />
				</attribute>
			</attributes>
		</itemtype>
	</itemtypes>
</items>
