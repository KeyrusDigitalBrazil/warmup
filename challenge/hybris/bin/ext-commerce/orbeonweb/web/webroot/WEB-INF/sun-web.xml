<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sun-web-app PUBLIC '-//Sun Microsystems, Inc.//DTD Sun ONE Application Server 7.0 Servlet
2.3//EN' 'http://www.sun.com/software/sunone/appserver/dtds/sun-web-app_2_3-0.dtd'>


<sun-web-app>
    <security-role-mapping>
        <role-name>administrator</role-name>
        <group-name>administrator</group-name>
    </security-role-mapping>

    <session-config>
        <session-manager/>
    </session-config>

    <resource-ref>
        <res-ref-name>jdbc/db</res-ref-name>
        <jndi-name>jdbc/db</jndi-name>
    </resource-ref>

    <class-loader delegate="false"/>

</sun-web-app>