{
  "name": "y-main",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test --watch false --progress=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "setup:jit": "npm install && tsc -p tsconfig.json",
    "build:jit": "tsc -p tsconfig.json",
    "watch:jit": "tsc -w -p tsconfig.json",
    "build:aot": "ngc -p tsconfig-aot.json && rollup -c rollup-config.js",
    "i18n": "ng-xi18n -p tsconfig-aot.json --i18nFormat=xlf",
    "build:en": "ngc -p tsconfig-aot.json --i18nFile=./src/locale/messages.en.xlf --locale=en --i18nFormat=xlf && rollup -c rollup-config.js --environment locale:en",
    "build:de": "ngc -p tsconfig-aot.json --i18nFile=./src/locale/messages.de.xlf --locale=de --i18nFormat=xlf && rollup -c rollup-config.js --environment locale:de",
    "build:ja": "ngc -p tsconfig-aot.json --i18nFile=./src/locale/messages.ja.xlf --locale=ja --i18nFormat=xlf && rollup -c rollup-config.js --environment locale:ja",
    "build:zh": "ngc -p tsconfig-aot.json --i18nFile=./src/locale/messages.zh.xlf --locale=zh --i18nFormat=xlf && rollup -c rollup-config.js --environment locale:zh",
    "build:all": "npm run build:en && npm run build:de && npm run build:ja && npm run build:zh"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/compiler-cli": "^4.1.2",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/platform-server": "^4.1.2",
    "@angular/router": "^4.0.0",
    "core-js": "^2.4.1",
    "ngx-cookie": "^1.0.0",
    "rxjs": "^5.1.0",
    "systemjs": "0.20.12",
    "zone.js": "0.8.13"
  },
  "devDependencies": {
    "@angular/cli": "1.0.2",
    "@angular/compiler-cli": "^4.0.0",
    "@types/jasmine": "2.5.38",
    "@types/node": "~6.0.60",
    "codelyzer": "~2.0.0",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-phantomjs-launcher": "~1.0.4",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.0",
    "rollup": "^0.41.6",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^1.0.2",
    "ts-node": "~2.0.0",
    "tslint": "~4.5.0",
    "typescript": "~2.3.0"
  }
}
